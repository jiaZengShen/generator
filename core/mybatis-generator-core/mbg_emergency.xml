<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<!-- 
		targetRuntime="MyBatis3Simple":生成简单版的CRUD
		MyBatis3:豪华版
	 -->
  <context id="DB2Tables" targetRuntime="MyBatis3">
  	<!-- jdbcConnection：指定如何连接到目标数据库-->
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://59.110.68.181:3306/EMERGENCY_MDB?useUnicode=true&amp;serverTimezone=UTC&amp;allowMultiQueries=true&amp;characterEncoding=UTF-8"
        userId="hozedata"
        password="Hozedata@123">
    </jdbcConnection>

      <!--
           <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                            connectionURL="jdbc:mysql://localhost:3306/emergency?useUnicode=true&amp;serverTimezone=UTC&amp;allowMultiQueries=true&amp;characterEncoding=UTF-8"
                            userId="root"
                            password="501821782">
            </jdbcConnection>
            -->
    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

	<!-- javaModelGenerator：指定javaBean的生成策略 
	targetPackage="test.model"：目标包名
	targetProject="\MBGTestProject\src"：目标工程
	-->
    <javaModelGenerator targetPackage="com.hozedata.emergency.system.pojo"
    		targetProject="d:\src\main\java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

	<!-- sqlMapGenerator：sql映射生成策略： -->
    <sqlMapGenerator targetPackage="mapper"
    	targetProject="d:\src\main\resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

	<!-- javaClientGenerator:指定mapper接口所在的位置 -->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.hozedata.emergency.system.mapper"
    	targetProject="d:\src\main\java">
        <property name="enableSubPackages" value="true" />
        <property name="daoTargetPackage" value="com.hozedata.emergency.system.dao"/>
        <property name="daoImplTargetPackage" value="com.hozedata.emergency.system.dao.impl"/>
        <property name="serviceTargetPackage" value="com.hozedata.emergency.system.service"/>
        <property name="serviceImplTargetPackage" value="com.hozedata.emergency.system.service.impl"/>
        <property name="controllerTargetPackage" value="com.hozedata.emergency.system.controller"/>
        <property name="controllerReturnType" value="com.hozedata.emergency.system.dto.AjaxResultMsgDTO"/>
    </javaClientGenerator>

	<!-- 指定要逆向分析哪些表：根据表要创建javaBean -->

      <table tableName="DIAGNOSIS" domainObjectName="Diagnosis" mapperName="DiagnosisMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="ID" sqlStatement="JDBC" />
      </table>


      <table tableName="DIAGNOSIS" domainObjectName="Diagnosis" mapperName="DiagnosisMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="ID" sqlStatement="JDBC" />
      </table>

      <table tableName="DUTY_DICT" domainObjectName="Duty" mapperName="DutyMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="ID" sqlStatement="JDBC" />
      </table>

      <table tableName="EVENT" domainObjectName="Event" mapperName="EventMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="ID" sqlStatement="JDBC" />
      </table>

      <table tableName="EVENT_RECORD" domainObjectName="EventRecord" mapperName="EventRecordMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="ID" sqlStatement="JDBC" />
      </table>

      <table tableName="FIRST_AID_INFO" domainObjectName="FirstAidInfo" mapperName="FirstAidInfoMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="ID" sqlStatement="JDBC" />
      </table>

      <table tableName="OPERATION" domainObjectName="Operation" mapperName="OperationMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="ID" sqlStatement="JDBC" />
      </table>

      <table tableName="OPERATION_SEGMENT" domainObjectName="OperationSegment" mapperName="OperationSegmentMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="ID" sqlStatement="JDBC" />
      </table>

      <table tableName="RESOURCE" domainObjectName="Resource" mapperName="ResourceMapper">
          <property name="useActualColumnNames" value="false"/>
      </table>

      <table tableName="PATIENT" domainObjectName="Patient" mapperName="PatientMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="PATIENT_ID" sqlStatement="JDBC" />
      </table>

      <table tableName="PATIENT_SELF_EVENT" domainObjectName="PatientSelfEvent" mapperName="PatientSelfEventMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="ID" sqlStatement="JDBC" />
      </table>


  </context>
</generatorConfiguration>
