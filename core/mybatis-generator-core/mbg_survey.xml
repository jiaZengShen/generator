<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<!-- 
		targetRuntime="MyBatis3Simple":生成简单版的CRUD
		MyBatis3:豪华版
	
	 -->
  <context id="DB2Tables" targetRuntime="MyBatis3">
  	<!-- jdbcConnection：指定如何连接到目标数据库 -->
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://192.168.199.183:3306/survey?useUnicode=true&amp;serverTimezone=UTC&amp;allowMultiQueries=true&amp;characterEncoding=UTF-8"
        userId="hozedata"
        password="Hozedata@123">
    </jdbcConnection>

	<!--  -->
    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

	<!-- javaModelGenerator：指定javaBean的生成策略 
	targetPackage="test.model"：目标包名
	targetProject="\MBGTestProject\src"：目标工程
	-->
    <javaModelGenerator targetPackage="com.hozedata.research.entity" 
    		targetProject="D:\temp\research-develop-tttttt\src\main\java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

	<!-- sqlMapGenerator：sql映射生成策略： -->
    <sqlMapGenerator targetPackage="mapper"
    	targetProject="D:\temp\research-develop-tttttt\src\main\resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

	<!-- javaClientGenerator:指定mapper接口所在的位置 -->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.hozedata.research.dao.mapper"
    	targetProject="D:\temp\research-develop-tttttt\src\main\java">
      <property name="enableSubPackages" value="true" />
      <property name="daoTargetPackage" value="com.hozedata.research.dao"/>
      <property name="daoImplTargetPackage" value="com.hozedata.research.dao.impl"/>
      <property name="serviceTargetPackage" value="com.hozedata.research.service"/>
      <property name="serviceImplTargetPackage" value="com.hozedata.research.service.impl"/>
      <property name="controllerTargetPackage" value="com.hozedata.research.controller"/>
      <property name="controllerReturnType" value="com.hozedata.research.entity.Message"/>
    </javaClientGenerator>

	<!-- 指定要逆向分析哪些表：根据表要创建javaBean -->
    <table tableName="t_answer" domainObjectName="Answer" mapperName="AnswerMapper">
    	<property name="useActualColumnNames" value="false"/>
      <generatedKey column="id" sqlStatement="JDBC" />
    </table>
    <table tableName="t_answer_item" domainObjectName="AnswerItem" mapperName="AnswerItemMapper">
    	<property name="useActualColumnNames" value="false"/>
      <generatedKey column="id" sqlStatement="JDBC" />
    </table>
      <table tableName="t_option_setting" domainObjectName="OptionSetting" mapperName="OptionSettingMapper"  >
          <property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
      </table>
    <table tableName="t_option" domainObjectName="Option" mapperName="OptionMapper">
    	<property name="useActualColumnNames" value="false"/>
      <generatedKey column="id" sqlStatement="JDBC" />
    </table>
      <table tableName="t_question" domainObjectName="Question" mapperName="QuestionMapper">
          <property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
      </table>
      <table tableName="t_question_group" domainObjectName="QuestionGroup"
             mapperName="QuestionGroupMapper"  >
          <property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
      </table>
      <!--快照表-->
      <table tableName="t_snapshot_option_setting" domainObjectName="OptionSetting" mapperName="SnapshotOptionSettingMapper"  >
          <property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
      </table>
      <table tableName="t_snapshot_option" domainObjectName="Option" mapperName="SnapshotOptionMapper">
          <property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
      </table>
      <table tableName="t_snapshot_question" domainObjectName="SnapshotQuestion" mapperName="SnapshotQuestionMapper">
          <property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
      </table>
      <table tableName="t_paper" domainObjectName="Paper" mapperName="PaperMapper">
          <property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
      </table>
    <table tableName="t_project" domainObjectName="Project" mapperName="ProjectMapper">
    	<property name="useActualColumnNames" value="false"/>
      <generatedKey column="id" sqlStatement="JDBC" />
    </table>




    <table tableName="t_survey" domainObjectName="Survey" mapperName="SurveyMapper">
    	<property name="useActualColumnNames" value="false"/>
      <generatedKey column="id" sqlStatement="JDBC" />
    </table>
    <table tableName="t_survey_logic" domainObjectName="SurveyLogic" mapperName="SurveyLogicMapper">
    	<property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
    </table>
      <table tableName="t_condition" domainObjectName="Condition" mapperName="ConditionMapper">
          <property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
      </table>

    <table tableName="t_user" domainObjectName="User" mapperName="UserMapper">
    	<property name="useActualColumnNames" value="false"/>
      <generatedKey column="id" sqlStatement="JDBC" />
    </table>
    
    
  </context>
</generatorConfiguration>
