        <property name="enableSubPackages" value="true" />
        <property name="daoTargetPackage" value="com.hozedata.fms.dao.performance"/>
        <property name="daoImplTargetPackage" value="com.hozedata.fms.dao.impl.performance"/>
        <property name="serviceTargetPackage" value="com.hozedata.fms.service.performance"/>
        <property name="serviceImplTargetPackage" value="com.hozedata.fms.service.impl.performance"/>
        <property name="controllerTargetPackage" value="com.hozedata.fms.controller.performance"/>
        <property name="controllerReturnType" value="com.hozedata.fms.dto.AjaxResultMsgDTO"/>
    </javaClientGenerator>

