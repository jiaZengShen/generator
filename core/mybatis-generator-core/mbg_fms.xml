<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<!-- 
		targetRuntime="MyBatis3Simple":生成简单版的CRUD
		MyBatis3:豪华版
	
	 -->
  <context id="DB2Tables" targetRuntime="MyBatis3">
  	<!-- jdbcConnection：指定如何连接到目标数据库 -->
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://192.168.199.183:3306/hoze_fms?useUnicode=true&amp;serverTimezone=UTC&amp;allowMultiQueries=true&amp;characterEncoding=UTF-8"
        userId="hozedata"
        password="Hozedata@123">
    </jdbcConnection>

	<!--  -->
    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

	<!-- javaModelGenerator：指定javaBean的生成策略 
	targetPackage="test.model"：目标包名
	targetProject="\MBGTestProject\src"：目标工程
	-->
    <javaModelGenerator targetPackage="com.hozedata.fms.entity" 
    		targetProject="d:\src\main\java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

	<!-- sqlMapGenerator：sql映射生成策略： -->
    <sqlMapGenerator targetPackage="mapper"
    	targetProject="d:\src\main\resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

	<!-- javaClientGenerator:指定mapper接口所在的位置 -->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.hozedata.fms.dao.mapper"  
    	targetProject="d:\src\main\java">
        <property name="enableSubPackages" value="true" />
        <property name="daoTargetPackage" value="com.hozedata.fms.dao"/>
        <property name="daoImplTargetPackage" value="com.hozedata.fms.dao.impl"/>
        <property name="serviceTargetPackage" value="com.hozedata.fms.service"/>
        <property name="serviceImplTargetPackage" value="com.hozedata.fms.service.impl"/>
        <property name="controllerTargetPackage" value="com.hozedata.fms.controller"/>
        <property name="controllerReturnType" value="com.hozedata.fms.entity.Message"/>
    </javaClientGenerator>

	<!-- 指定要逆向分析哪些表：根据表要创建javaBean -->
    <table tableName="SYS_ROLE" domainObjectName="Role" mapperName="RoleMapper">
    	<property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
    </table>
    <table tableName="SYS_ROLE_AUTHORITY" domainObjectName="RoleAuthority" mapperName="RoleAuthorityMapper">
    	<property name="useActualColumnNames" value="false"/>
        <generatedKey column="AUTHORITY_ID" sqlStatement="JDBC" />
    </table>
    <table tableName="SYS_ROLE_AUTHORITY_RELATION" domainObjectName="RoleAuthorityRelation" mapperName="RoleAuthorityRelationMapper">
    	<property name="useActualColumnNames" value="false"/>
    </table>
    <table tableName="SYS_ROLE_DIMENSION" domainObjectName="RoleDimension" mapperName="RoleDimensionMapper">
    	<property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
    </table>
    
    <table tableName="SYS_USER" domainObjectName="User" mapperName="UserMapper">
    	<property name="useActualColumnNames" value="false"/>
        <generatedKey column="USER_ID" sqlStatement="JDBC" />
    </table>
    
    <table tableName="SYS_USER_AUTHORITY" domainObjectName="UserAuthority" mapperName="UserAuthoritypper">
    	<property name="useActualColumnNames" value="false"/>
        <generatedKey column="AUTHORITY_ID" sqlStatement="JDBC" />
    </table>
    
    <table tableName="SYS_USER_DIMENSION" domainObjectName="UserDimension" mapperName="UserDimensionMapper">
    	<property name="useActualColumnNames" value="false"/>
        <generatedKey column="id" sqlStatement="JDBC" />
    </table>
     <table tableName="ROLE_OBJECT_TEMPLATE" domainObjectName="UserDimension" mapperName="UserDimensionMapper">
    	<property name="useActualColumnNames" value="false"/>
         <generatedKey column="id" sqlStatement="JDBC" />
    </table>
     <table tableName="SYS_USER_DIMENSION" domainObjectName="UserDimension" mapperName="UserDimensionMapper">
    	<property name="useActualColumnNames" value="false"/>
         <generatedKey column="id" sqlStatement="JDBC" />
    </table>
      <table tableName="SYS_USER_LOGIN_HISTORY" domainObjectName="UserLoginAction" mapperName="UserLoginHistoryMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="id" sqlStatement="JDBC" />
      </table>
      <table tableName="SYS_USER_LOGIN" domainObjectName="UserLoginAction" mapperName="UserLoginMapper">
          <property name="useActualColumnNames" value="false"/>
          <generatedKey column="id" sqlStatement="JDBC" />
      </table>



  </context>
</generatorConfiguration>
